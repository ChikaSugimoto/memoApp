<h3>New page</h3>
<%= form_tag("/memos") do %>
    <div class="form-group">
        <%= label_tag "form-title","Title"  %>
        <%= text_field :memos,:title,id:"form-title",class:"form-control",placeholder:"Title" %>
    </div>
    <div class="form-group">
        <%= label_tag "form-content","Content"  %>
        <%= text_field :memos,:body,id:"form-content",class:"form-control",placeholder:"Content" %>
    </div>
    <%= submit_tag "Create",class:"btn btn-default" %>
<% end %>